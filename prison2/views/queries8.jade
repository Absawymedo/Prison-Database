extends layout

block content
    h1= title

    form(method='POST' action='/queries/8')
    table(class='table table-striped table-bordered table-hover')
        thead 
            tr
                th CellBlock Name
                th Cell ID
                th Number of Inmates
                each result in results
                    tr
                        td= result.CBname
                        td= result.CID
                        td= result.NoInmates
    div
        button.btn.btn-back(type='button', onclick='history.back()') Go Back