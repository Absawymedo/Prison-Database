extends layout

block content
    h1= title
    table(class='table table-striped table-bordered table-hover')
        thead 
            tr
                th Cell Block Name
                th Cell Block Type
                th Number of Cameras
                each result in results
                    tr
                        td= result.CBname
                        td= result.CBType
                        td= result.NoCams
    div
        button.btn.btn-back(type='button', onclick='history.back()') Go Back